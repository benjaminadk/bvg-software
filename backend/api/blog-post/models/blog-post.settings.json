{
  "kind": "collectionType",
  "collectionName": "blog_posts",
  "info": {
    "name": "Blog Post",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "attributes": {
    "meta_title": {
      "type": "string"
    },
    "meta_description": {
      "type": "text"
    },
    "content": {
      "type": "richtext"
    },
    "title": {
      "type": "string"
    },
    "slug": {
      "type": "string"
    },
    "image": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "published_on": {
      "type": "date"
    },
    "read_time": {
      "type": "string"
    },
    "video": {
      "type": "component",
      "repeatable": false,
      "component": "multimedia.video"
    },
    "tags": {
      "type": "string"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "draft",
        "publish"
      ]
    }
  }
}
